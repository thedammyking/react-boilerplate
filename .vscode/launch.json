{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "port": 9223,
      "runtimeExecutable": "chrome.exe",
      "runtimeArgs": ["--remote-debugging-port=9223"],
      "userDataDir": false,
      "webRoot": "${workspaceRoot}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      },
      "skipFiles": ["node_modules/**"]
    }
  ]
}
